<Application x:Class="TwitchDesktopApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TwitchDesktopApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Power" UriSource="pack://application:,,,/TwitchDesktopApp;component/Assets/power.png"></BitmapImage>
            <BitmapImage x:Key="Home" UriSource="./Assets/home.png"></BitmapImage>
        </ResourceDictionary>
         
    </Application.Resources>
</Application>
